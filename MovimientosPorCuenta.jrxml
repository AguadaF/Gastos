	<queryString language="SQL">
		<![CDATA[SELECT cuentas.nombre,
	fecha,
	importe,
	concepto
FROM movimien
	left join cuentas ON 
	 movimien.cuentaid = cuentas.id 
WHERE 
     movimien.fecha between  $P{pdefec}  and  $P{phafec}  and 
	 movimien.cuentaid =  $P{pcuenta} 
ORDER BY fecha asc]]>
	</queryString>
